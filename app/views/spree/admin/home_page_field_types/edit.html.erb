<% content_for :page_title do %>
  <%= link_to Spree.t(:home_page_field_types), spree.admin_home_page_field_types_url %> /
  <%= @home_page_field_type.name %>
<% end %>

<%= render partial: 'spree/admin/shared/error_messages', locals: { target: @home_page_field_type } %>

<%= form_for [:admin, @home_page_field_type] do |f| %>
  <fieldset>
    <%= render partial: 'form', locals: { f: f } %>
    <%= render partial: 'spree/admin/shared/edit_resource_links' %>
  </fieldset>
<% end %>
